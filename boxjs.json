                    "placeholder": "YouTube的策略组名，需要是select类型",
                    "autoGrow": true,
                    "rows": 1
                },
                {
                    "id": "@youtube.region",
                    "val": "CN",
                    "name": "所需地区",
                    "type": "textarea",
                    "placeholder": "需要选择的地区",
                    "autoGrow": true,
                    "rows": 1
                }
            ]
        },
        {
            "id": "auto-plocy",
            "name": "自动策略组切换",
            "keys": [
                "@auto_policy.policies",
                "@auto_policy.api",
                "@auto_policy.key"
            ],
            "author": "@Skyxim",
            "settings": [
                {
                    "id": "@auto_policy.policies",
                    "name": "策略列表",
                    "val": "",
                    "type": "textarea",
                    "placeholder": "",
                    "autoGrow": true,
                    "rows": 5
                }
            ]
        },
        {
            "id": "holiday",
            "name": "节假日数据",
            "keys": [
                "@work_cache.month"
            ],
