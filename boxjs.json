{
    "id": "skyxim",
    "name": "Skyxim",
    "author": "@Skyxim",
    "repo": "https://github.com/Skyxim/Profile",
    "apps": [
        {
            "id": "youtube",
            "name": "YouTube",
            "keys": [
                "@youtube.policy",
                "@youtube.region"
            ],
            "repo": "https://github.com/Skyxim/Profile",
            "script": "https://github.com/Skyxim/Profile/raw/main/Script/YouTubeSelect.js",
            "settings": [
                {
                    "id": "@youtube.policy",
                    "name": "YouTube策略组",
